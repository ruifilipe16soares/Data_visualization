{
  "name": "mysql-connector-chtv",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "database.hostname": "${file:/secrets/mysql.properties:host}",
    "database.port": "3306",
    "database.user": "${file:/secrets/mysql.properties:user}",
    "database.password": "${file:/secrets/mysql.properties:password}",
    "database.server.id": "243446",
    "database.include.list": "chtv",
    "database.history.kafka.bootstrap.servers": "kafka:9092",
    "table.exclude.list": "chtv.shedlock,chtv.agrupa_tipo_pedido,chtv.DATABASECHANGELOG,chtv.DATABASECHANGELOGLOCK",
    "schema.history.internal.kafka.topic": "schema-history-chtv",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:9092",
    "include.schema.changes": "true",
    "topic.prefix": "mysql-chtv",
    "snapshot.mode": "initial",
    "database.history.skip.unparseable.ddl": "true",
    "database.history.store.only.monitored.tables.ddl": "true",
    "time.precision.mode": "connect",
    "database.connectionTimeZone": "Europe/Lisbon"
  }
}

